{
	"name": "urna",
	"version": "1.0.0",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"dev": "run-p dev:*",
		"dev:frontend": "cd apps/frontend && npm run dev",
		"dev:backend": "cd apps/backend && npm run start:dev",
		"dev:auth": "cd packages/auth && npm run dev",
		"dev:core": "cd packages/core && npm run dev"
	},
	"dependencies": {
		"npm-run-all": "^4.1.5"
	}
}
